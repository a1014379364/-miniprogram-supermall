<!--pages/detail/ParamsInfo/ParamsInfo.wxml-->
<view class="sizes">
  <view wx:for="{{paramsInfo.sizes[0]}}" class="row">
    <text class="col" wx:for="{{item}}">{{item}}</text>
  </view>
</view>
<view class="info">
  <view wx:for="{{paramsInfo.info}}" class="row">
    <text class="key">{{item.key}}</text>
    <text class="value">{{item.value}}</text>
  </view>
</view>
